{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext", // Ensure this is set to "NodeNext"
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,    
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}




      

